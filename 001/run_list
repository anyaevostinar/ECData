set description  001-config-baseline5
set email hjg@cse.msu.edu
set freq_email Crash
set priority 3
set class_pref 6,7,5,4,3
#set class_pref 100
set walltime 60 
set mem_request 2 
set config_dir config
set max_cpus 90

1..50 a ./avida -s $seed -c avida.cfg -set WORLD_X 5 -set WORLD_Y 2000 -set WORLD_GEOMETRY 2 -set SPECULATIVE 0 -set INST_SET kitchensink-instset-self.cfg -set EVENT_FILE events-long.cfg -set ENVIRONMENT_FILE environment-out.cfg -set START_CREATURE not-self.org -set NUM_DEMES 400 -set DEMES_USE_GERMLINE 1 -set DEMES_REPLICATE_SIZE 1 -set DEMES_ORGANISM_SELECTION 0 -set DEMES_ORGANISM_PLACEMENT 1 -set DEMES_ORGANISM_FACING 1 -set SLICING_METHOD 1  -set COPY_MUT_PROB 0 -set DIVIDE_INS_PROB 0 -set DIVIDE_DEL_PROB 0 -set DEMES_PREVENT_STERILE 1 -set TASK_SWITCH_PENALTY 0 -set TASK_SWITCH_PENALTY_TYPE 2 -set RES_FOR_DEME_REP 500 -set DEMES_TRACK_SHANNON_INFO 1 -set INST_SET_LOAD_LEGACY 1


